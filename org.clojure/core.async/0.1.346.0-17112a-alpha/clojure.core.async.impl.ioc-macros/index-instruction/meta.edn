{:ns "clojure.core.async.impl.ioc-macros", :name "index-instruction", :file "clojure/core/async/impl/ioc_macros.clj", :type :fn, :src "(defn index-instruction [blk-id idx inst]\n  (let [idx (reduce\n             (fn [acc id]\n               (update-in acc [id :read-in] (fnil conj #{}) blk-id))\n             idx\n             (filter instruction? (reads-from inst)))\n        idx (reduce\n             (fn [acc id]\n               (update-in acc [id :written-in] (fnil conj #{}) blk-id))\n             idx\n             (filter instruction? (writes-to inst)))]\n    idx))", :column 1, :line 813, :arglists ([blk-id idx inst])}